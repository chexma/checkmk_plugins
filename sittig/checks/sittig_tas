def inventory_sittig_tas(info):
    return [(line[0], None) for line in info]

def check_sittig_tas(item, params, info):
    for line in info:
        if line[0] == item:
            if line[1:] == [ "2", "2", "3" ]: # online, online, master-active
                return (0, "OK - online, Master-Active")
        elif line[1:] == [ "2", "2", "6" ]: # online, online, slave-inactive
            return (0, "OK - online, Slave-Inactive")
        else:
            return (2, "CRIT - offline or partially offline")  
    return (3, 'UNKNOWN - TAS not found')

check_info['sittig_tas'] = (check_sittig_tas, "TAS %s", 0, inventory_sittig_tas)

snmp_info['sittig_tas'] = ( ".1.3.6.1.4.1.33472.4.18.3.1", [0, 1, 2, 3 ] )

snmp_scan_functions['sittig_tas'] = \
    lambda oid: oid(".1.3.6.1.2.1.1.2.0") == ".1.3.6.1.4.1.311.1.1.3.1.2"
