def inventory_sittig_rsa(info):
    return [(line[0], None) for line in info]


def check_sittig_rsa(item, params, info):
    for line in info:
        if line[0] == item:
            if line[1:] == [ "2", "2", "3", "2", "2", "3" ]: # online, online, n-a, online, online, master-active
                return (0, "OK - online, Master-Active")
            elif line[1:] == [ "2", "2", "3", "2", "2", "6" ]: # online, online, n-a, online, online, slave-inactive
                return (0, "OK - online, Slave-Inactive")
            else:
                return (2, "CRIT - offline or partially offline")

    return (3, 'UNKNOWN - RSA not found')

check_info['sittig_rsa'] = (check_sittig_rsa, "RSA %s", 0, inventory_sittig_rsa)

snmp_info['sittig_rsa'] = ( ".1.3.6.1.4.1.33472.4.17.3.1", [ 0, 1, 2, 3, 4, 5, 6 ] )

snmp_scan_functions['sittig_rsa'] = \
    lambda oid: oid(".1.3.6.1.2.1.1.2.0") == ".1.3.6.1.4.1.311.1.1.3.1.2"